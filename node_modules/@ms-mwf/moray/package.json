{
  "name": "@ms-mwf/moray",
  "version": "1.9.2",
  "description": "Microsoft Web Framework based on Bootstrap 4",
  "repository": {
    "type": "git",
    "url": "https://mscomdev.visualstudio.com/Moray/_git/Moray"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": ">=14.17.0",
    "npm": ">=7.14.0"
  },
  "files": [
    "/dist/css",
    "/dist/fonts",
    "/dist/js",
    "/fractal",
    "!/fractal/build",
    "/src",
    "fractal.js"
  ],
  "scripts": {
    "dev": "gulp",
    "ci-snapshots": "start-server-and-test serve 9001 snapshot",
    "cypress": "cypress run",
    "cypress-ci": "cypress run --config baseUrl=https://moray-$SOURCEBRANCHNAME.azurewebsites.net",
    "cypress-open": "cypress open",
    "cypress-percy": "percy exec -- cypress run",
    "build": "cross-env NODE_ENV=production gulp build",
    "build-staging": "cross-env NODE_ENV=staging gulp build",
    "build-fractal": "cross-env NODE_ENV=production gulp buildFractal",
    "lint": "run-p lint-css lint-js",
    "lint-css": "stylelint \"src/**/*.scss\"",
    "lint-js": "eslint . --ext .js,.json",
    "lint-js-fix": "npm run lint-js -- --fix",
    "test": "jest",
    "test-coverage": "npm-run-all -p test cypress -s coverage",
    "coverage-check": "nyc report --check-coverage --reporter=text",
    "coverage-merge": "cypress-jest-coverage-merge",
    "precoverage": "del .nyc_output tests/coverage/reports",
    "coverage": "run-s coverage-merge coverage-check",
    "bundlesize": "bundlesize",
    "test-bundlesize": "run-s build bundlesize",
    "percy": "percy snapshot ./percy/pages.js --config ./percy/.percy.yml",
    "prepare": "npx is-ci || npx husky install",
    "serve": "serve -l 9001 ./fractal/build",
    "setup": "npm i && chmod +x new-component.sh",
    "snapshot": "run-s cypress-percy percy"
  },
  "dependencies": {
    "core-js": "^3.14.0",
    "dom4": "^2.1.6",
    "imagesloaded": "^4.1.4",
    "intersection-observer": "^0.12.0",
    "picturefill": "^3.0.3",
    "smoothscroll-polyfill": "^0.4.4",
    "throttle-debounce": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/eslint-parser": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@cypress/code-coverage": "^3.9.6",
    "@cypress/skip-test": "^2.6.1",
    "@frctl/fractal": "^1.5.8",
    "@frctl/handlebars": "^1.2.10",
    "@frctl/mandelbrot": "^1.9.2",
    "@ms-mwf/babel-config": "0.0.1",
    "@ms-mwf/browserslist-config": "0.0.3",
    "@ms-mwf/eslint-config": "0.0.8",
    "@ms-mwf/prettier-config": "0.0.3",
    "@ms-mwf/stylelint-config": "0.0.3",
    "@percy/cli": "^1.0.0-beta.60",
    "@percy/cypress": "^3.1.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "autoprefixer": "^10.2.6",
    "babel-jest": "^27.0.2",
    "babel-plugin-istanbul": "^6.0.0",
    "bundlesize": "^0.18.1",
    "cross-env": "^7.0.3",
    "cypress": "^8.7.0",
    "cypress-fail-on-console-error": "^2.1.2",
    "cypress-jest-coverage-merge": "^1.0.2",
    "cypress-real-events": "^1.5.1",
    "cypress-terminal-report": "^3.3.2",
    "del": "^6.0.0",
    "del-cli": "^4.0.0",
    "eslint": "^7.28.0",
    "eslint-config-xo": "^0.36.0",
    "eslint-plugin-chai-friendly": "^0.7.1",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-unicorn": "^33.0.1",
    "event-stream": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-header": "^2.0.9",
    "gulp-if": "^3.0.0",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.0.1",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "merge-stream": "^2.0.0",
    "node-sass-once-importer": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.4",
    "rollup": "^2.52.0",
    "rollup-plugin-livereload": "^2.0.0",
    "serve": "^12.0.0",
    "start-server-and-test": "^1.12.5",
    "stylelint": "^13.13.1",
    "stylelint-config-twbs-bootstrap": "^2.2.1"
  },
  "prettier": "@ms-mwf/prettier-config"
}
