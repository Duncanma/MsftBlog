{{#each group as |value key|}}
  <div class="{{../prefix}}-{{value}} bg-gray-100">Float {{key}}</div><br>
{{/each}}

{{#each breakpoints as |bp bpKey|}}
  {{#each ../group as |value key|}}
    <div class="{{../../prefix}}-{{bp}}-{{value}} bg-gray-100">Float {{key}} at {{bpKey}}</div><br>
  {{/each}}
{{/each}}
