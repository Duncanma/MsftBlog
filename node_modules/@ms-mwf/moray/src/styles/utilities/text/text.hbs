{{#each examples}}
<code>.{{../prefix}}-{{utility}}</code>
<div class="bg-neutral-100 p-2{{#unless @last}} mb-3{{/unless}}">
  <{{#unless custom-tag}}p{{else}}{{custom-tag}}{{/unless}} class="
    {{~../prefix}}-{{utility~}}
    {{~#if example-classes}} {{example-classes}}{{/if~}}
    {{~#unless custom-tag}} mb-0{{/unless~}}"
    {{~#if attributes}} {{{htmlAttr attributes}}}{{/if~}}>
    {{{content}}}
  </{{#unless custom-tag}}p{{else}}{{custom-tag}}{{/unless}}>
</div>
{{/each}}
