<div class="row no-gutters mb-3">

  {{#each materials as |value key|}}
  <div class="{{#if ../column.classes}}{{../column.classes}}{{else}}col{{/if}} p-1">
    <!-- material-{{#if ../bp}}{{../bp}}-{{/if}}{{value}} -->
    <div class="material-{{#if ../bp}}{{../bp}}-{{/if}}{{#if ../prefix}}{{../prefix}}-{{/if}}{{value}} h-100 p-2">
      <p>{{key}}{{#if ../bpKey}} at {{../bpKey}}{{/if}}</p>
      {{render '@link-group--1-link-1-cta'}}
    </div>
  </div>
  {{/each}}

</div>