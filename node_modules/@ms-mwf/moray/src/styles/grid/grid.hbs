

{{#each grid.containers}}
<{{#if this.el}}{{this.el}}{{else}}div{{/if}} class="container{{#if this.fluid}}-fluid{{/if}} {{this.classes}}">
  {{#each this.rows}}
  <{{#if this.el}}{{this.el}}{{else}}div{{/if}} class="row {{this.classes}}{{#if this.no-gutters}} no-gutters{{/if}}">
    {{#each this.cols}}
    <{{#if this.el}}{{this.el}}{{else}}div{{/if}}
      class="col{{#if this.col}}-{{this.col}}{{/if}}{{#each this.breakpoints}} col-{{this.breakpoint}}-{{this.col}}{{/each}} {{this.classes}}">
      {{{this.content}}}
    </{{#if this.el}}{{this.el}}{{else}}div{{/if}}>
    {{/each}}
  </{{#if this.el}}{{this.el}}{{else}}div{{/if}}>
  {{/each}}
</{{#if this.el}}{{this.el}}{{else}}div{{/if}}>
{{/each}}
