{
  "status": "ready",
  "default": "default",
  "collated": false,
  "title": "Banner Carousel",
  "handle": "carousel-banner",
  "preview": "@preview-grid",
  "context": {
    "classes": "carousel-lg-side-controls carousel-variable-height",
    "aria-roledescription": "slideshow",
    "previous-label": "Previous slide",
    "next-label": "Next slide",
    "aria-label": "Microsoft Quotes",
    "skip-link": {
      "text": "Skip Microsoft Quotes"
    },
    "skip-end-text": "End of Microsoft Quotes section",
    "status": {
      "container-id": "status-container",
      "sr-element-id": "status-msg",
      "sr-text": "Slide %{start} of %{total}. %{slideTitle}"
    },
    "slides": [
      {
        "aria-roledescription": "slide",
        "aria-label": "1 of 3",
        "handle": "\\@banner--rounded-image"
      },
      {
        "aria-roledescription": "slide",
        "aria-label": "2 of 3",
        "handle": "\\@banner--rounded-image"
      },
      {
        "aria-roledescription": "slide",
        "aria-label": "3 of 3",
        "handle": "\\@banner--rounded-image"
      }
    ]
  },
  "variants": [
    {
      "name": "default"
    }
  ]
}
