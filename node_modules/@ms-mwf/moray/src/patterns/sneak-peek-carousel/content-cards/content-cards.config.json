            },
            {
              "name": "paragraphs",
              "data": {
                "classes": "mb-3",
                "content": "New features in Word, PowerPoint, and Excel combined with the productivity tools of Windows give you just what you need to achieve more, without doing more."
              }
            }
          ]
        },
        "footer": {
          "templates": [
            {
              "name": "link-group",
