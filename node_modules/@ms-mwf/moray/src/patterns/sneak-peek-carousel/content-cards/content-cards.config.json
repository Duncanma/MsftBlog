{
  "status": "ready",
  "default": "default",
  "collated": false,
  "title": "Content Cards Sneak Peek",
  "handle": "carousel-content-cards",
  "preview": "@preview-grid",
  "context": {
    "carousel": true,
    "classes": "carousel-content-cards carousel-sneak-peek",
    "aria-roledescription": "slideshow",
    "previous-label": "Previous slide",
    "next-label": "Next slide",
    "aria-label": "Windows 10 features",
    "skip-link": {
      "text": "Skip Windows 10 features"
    },
    "skip-end-text": "End of Windows 10 features section",
    "area-heading": {
      "classes": "mb-5",
      "centered": true,
      "headings": {
        "html-tag": "h2",
        "text": "The best Windows keeps getting better"
      },
      "paragraphs": {
        "content": "Designed to make staying up-to-date easy, Windows 10 regularly provides new features and updated security, at no additional cost."
      },
      "has-body": true
    },
    "status": {
      "container-id": "status-container",
      "sr-element-id": "status-msg",
      "sr-text": "Slide %{start} of %{total}. %{slideTitle}"
    },
    "slides": [
      {
        "aria-roledescription": "slide",
        "aria-label": "1 of 3 + (optional) Placeholder for explicit label",
