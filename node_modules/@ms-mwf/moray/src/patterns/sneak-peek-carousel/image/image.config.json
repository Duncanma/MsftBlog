{
  "status": "ready",
  "default": "default",
  "collated": false,
  "title": "Image Sneak Peek",
  "handle": "image-sneak-peek",
  "preview": "@preview-grid",
  "context": {
    "classes": "carousel-highlight carousel-sneak-peek",
    "aria-roledescription": "slideshow",
    "previous-label": "Previous slide",
    "next-label": "Next slide",
    "aria-label": "Xbox offers",
    "skip-link": {
      "text": "Skip Xbox offers"
    },
    "skip-end-text": "End of Xbox offers section",
    "area-heading": {
      "headings": {
        "html-tag": "h2",
        "classes": "h2",
        "text": "Surface Go is our smallest, lightest Surface yet"
      },
      "paragraphs": {
        "classes": "pb-3 pb-lg-0",
        "content": "Surface offers new ways to work, play, and create Get to know these versatile devices that can adapt to your needs"
      },
      "has-body": true
    },
    "image-gallery": {
      "trigger": {
        "text": "See all 4 images",
        "glyph": {
          "position": "prepend",
          "name": "photo-collection"
        },
        "attributes": {
          "data-target": "#image-sneak-peek"
        }
      },
      "modal": {
        "id": "image-sneak-peek",
        "button": false,
        "label": "Image Sneak Peek",
        "title": "Xbox Offers",
        "type": "scrollable",
        "size": "xl",
        "body-header": {
          "content": "4 images"
        },
        "images": [
          {
            "src": "https://morayassets.z21.web.core.windows.net/images/carousel/Slide1_1260x600.jpg",
            "altText": "Image 1"
          },
          {
            "src": "https://morayassets.z21.web.core.windows.net/images/carousel/Slide2_1260x600.jpg",
            "altText": "Image 2"
          },
          {
            "src": "https://morayassets.z21.web.core.windows.net/images/carousel/Slide3_1260x600.jpg",
            "altText": "Image 3"
          },
          {
            "src": "https://morayassets.z21.web.core.windows.net/images/carousel/Slide4_1260x600.jpg",
            "altText": "Image 4"
          }
        ],
        "footer": false
      }
    },
    "status": {
      "container-id": "status-container",
      "sr-element-id": "status-msg",
      "sr-text": "Slide %{start} of %{total}. %{slideTitle}"
    },
    "slides": [
