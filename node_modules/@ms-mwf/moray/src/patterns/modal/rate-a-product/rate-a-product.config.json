{
  "title": "Rate a product",
  "status": "ready",
  "handle": "rate-a-product",
  "hidden": true,
  "context": {
    "needs-init": true,
    "product-name": "Surface Pro 6 + Type Cover",
    "id": "modal-rate-and-review",
    "aria-labelledby-id": "modal-rate-and-review-title",
    "body": {
      "classes": "px-5"
    },
    "footer": {
      "classes": "py-4 mt-3"
    }
  },
  "variants": [
    {
      "name": "default",
      "hidden": true,
      "context": {
        "needs-init": false,
        "demo": true
      }
    },
    {
      "name": "rate",
      "context": {
        "template": "\\@rate-a-product-content--rate"
      }
    },
    {
      "name": "rate-error",
      "context": {
        "template": "\\@rate-a-product-content--rate-error"
      }
    },
    {
      "name": "review",
      "context": {
        "template": "\\@rate-a-product-content--review"
      }
    },
    {
      "name": "review-error",
      "context": {
        "template": "\\@rate-a-product-content--review-error"
      }
    },
    {
      "name": "confirmation",
      "context": {
        "template": "\\@rate-a-product-content--confirmation"
      }
    },
    {
      "name": "update",
      "context": {
        "template": "\\@rate-a-product-content--update"
      }
    }
  ]
}
