<div class="row">
  <div class="col-12">    
    {{#each content}}
      {{{this}}}
    {{/each}}
  </div>
</div>

<div class="row">

  <!-- Footnotes -->
  <section class="col-12 col-md-10 col-lg-8 mt-5" role="region" aria-label="footnotes: disclaimers">
    <ul class="list-unstyled small mb-0">
      {{#each footnotes}}
        <li class="py-2"><a id="{{link.id}}" href="{{link.href}}" aria-label="{{link.aria-label}}">{{link.text}}</a> {{{content}}}</li>
      {{/each}}
    </ul>
  </section>

</div>
