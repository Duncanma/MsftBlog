{{#if isDescriptionList}}
<div class="row">
    {{#each content}}
    <dt class="col-sm-3">{{dt}}</dt>
    <dd class="col-sm-9">
    {{#if dl}}
        <dl class="row">
            <dt class="col-sm-4">{{dl.dt}}</dt>
            <dd class="col-sm-8">{{{dl.dd}}}</dd>
        </dl>
    {{else}}
        {{{dd}}}
    {{/if}}
    </dd>
    {{/each}}
</div>
{{else}}

{{#if isUnordered}}
<ul {{#if listClassname}}class="{{listClassname}}"{{/if}}>
{{/if}}
{{#if isOrdered}}
<ol>
{{/if}}
    {{#each list-items}}
    <li {{#if ../listItemClassname}}class="{{../listItemClassname}}"{{/if}}>
      {{#if content}}
        {{{content}}}
      {{else}}
        {{> @partial-block }}
      {{/if}}
    </li>
    {{/each}}
{{#if isOrdered}}
</ol>
{{/if}}
{{#if isUnordered}}
</ul>
{{/if}}

{{/if}}