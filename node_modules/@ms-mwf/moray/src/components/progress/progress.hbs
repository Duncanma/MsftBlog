{{#if label.before}}<span class="d-inline-block mb-2" {{{htmlAttr label.before.attributes}}}>{{label.before.text}}</span>{{/if}}
{{#if progress-bar}}<div role="progressbar" aria-valuenow="{{valuenow}}" aria-valuemin="{{valuemin}}" aria-valuemax="{{valuemax}}" aria-label="{{aria-label}}">{{/if}}
  <div class="progress {{classes}}{{#if track-border-color}} border-{{track-border-color}}{{/if}}{{#if track-color}} bg-{{track-color}}{{/if}}"{{#if height}} style="height:{{height}};"{{/if}}>
    <div class="progress-bar{{#if bar-color}} bg-{{bar-color}}{{/if}}" style="width: {{math (math valuenow '/' (math valuemax '-' valuemin)) '*' 100}}%;"></div>
  </div>
{{#if progress-bar}}</div>{{/if}}
{{#if label.after}}<span class="d-inline-block mt-2">{{label.after.text}}</span>{{/if}}
