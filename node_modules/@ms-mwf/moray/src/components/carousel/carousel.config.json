{
  "status": "ready",
  "default": "default",
  "collated": false,
  "context": {
    "classes": "carousel-highlight carousel-variable-height",
    "aria-label": "Placeholder for explicit slideshow label",
    "aria-roledescription": "slideshow",
    "previous-label": "Previous slide",
    "next-label": "Next slide",
    "skip-link": {
      "text": "Skip placeholder for explicit slideshow label",
      "classes": "sr-only-focusable w-100 position-absolute"
    },
    "skip-end-text": "End of placeholder for explicit slideshow label section",
    "status": {
      "container-id": "status-container",
      "sr-element-id": "status-msg",
      "sr-text": "Slide %{start} of %{total}. %{slideTitle}"
    },
    "slides": [
      {
        "aria-roledescription": "slide",
        "aria-label": "1 of 3 + (optional) Placeholder for explicit label",
        "slide-content": "<img class=\"d-block w-100\" src=\"https://via.placeholder.com/690x345/757575?text=First+slide\" alt=\"First slide\">"
      },
      {
        "aria-roledescription": "slide",
        "aria-label": "2 of 3 + (optional) Placeholder for explicit label",
        "slide-content": "<img class=\"d-block w-100\" src=\"https://via.placeholder.com/690x345/757575?text=Second+slide\" alt=\"Second slide\">"
      },
      {
        "aria-roledescription": "slide",
        "aria-label": "3 of 3 + (optional) Placeholder for explicit label",
        "slide-content": "<img class=\"d-block w-100\" src=\"https://via.placeholder.com/690x345/757575?text=Third+slide\" alt=\"Third slide\">"
      }
    ]
  },
  "variants": [
    {
      "name": "default"
    },
    {
      "name": "no-loop",
      "context": {
        "no-loop": true
      }
    },
    {
      "name": "back-to-controls",
      "context": {
        "no-loop": true,
        "back-to-controls": {
          "text": "Back to Next and Previous controls",
          "classes": "back-to-controls text-left sr-only-focusable w-100 position-absolute bottom-2"
        }
      }
    }
  ]
}
