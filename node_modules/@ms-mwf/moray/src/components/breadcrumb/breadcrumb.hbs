
<nav aria-label="{{ariaLabel}}">
    <ol class="breadcrumb">
    {{#each listItem}}
        {{#if isActive}}
        <li class="breadcrumb-item active" aria-current="page">{{text}}</li>
        {{else}}
        <li class="breadcrumb-item"><a href="#">{{text}}</a></li>
        {{/if}}
    {{/each}}
    </ol>
</nav>