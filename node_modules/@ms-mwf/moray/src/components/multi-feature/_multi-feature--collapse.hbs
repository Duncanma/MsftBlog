<div class="multi-feature-body">
  {{#each body-templates}}
    {{render (strConcat '@' name) data merge=true}}
  {{/each}}
</div>
{{#if feature-templates}}
<div class="multi-feature-feature">
  {{#each feature-templates}}
    {{render (strConcat '@' name) data merge=true}}
  {{/each}}
</div>
{{/if}}