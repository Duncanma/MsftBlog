{
  "title": "Example pages",
  "status": "prototype",
  "default": "homepage",
  "preview": "@preview",
  "collated": false,
  "order": 5,
  "meta": {
    "uhf": true,
    "wrapper": {
      "no-margin": true
    }
  },
  "context": {
    "meta": {
      "main": {
        "attributes": {
          "id": "mainContent"
        }
      }
    },
    "templates": [
      {
        "name": "carousel-highlight-full-bleed",
        "full-bleed": true
      },
      {
        "name": "link-list--images-medium"
      },
      {
        "name": "content-cards-vertical--4-up-no-depth"
      },
      {
        "name": "highlight--small-card"
      },
      {
        "name": "content-cards-vertical--3-up-depth"
      },
      {
        "name": "feature--no-image"
      },
      {
        "name": "social--follow-horizontal",
        "container-classes": "mb-0"
      },
      {
        "name": "back-to-top--standalone",
        "container-classes": "mb-0"
      }
    ]
  },
  "variants": [
    {
      "name": "homepage"
    },
    {
      "name": "store-category-page",
      "context": {
        "templates": [
          {
            "name": "alert--announcement-bar-clickable",
            "full-bleed": true
          },
          {
            "name": "feature--mini-features"
          },
          {
            "name": "link-list--images-medium"
          },
          {
            "name": "carousel-product-card--devices",
            "data": {
              "status": {
                "container-id": "status-container-1",
                "sr-element-id": "status-msg-1",
                "sr-text": "Showing %{start} through %{end} of %{total} items",
                "visible-text": "Showing %{start}%{separator}%{end} of %{total} items"
              },
              "slides": [
                {
                  "aria-roledescription": "slide",
                  "aria-labelledby": "status-msg-1",
                  "handle": "\\@product-cards--devices",
                  "section": null,
                  "carousel": true
                },
                {
                  "aria-roledescription": "slide",
                  "aria-labelledby": "status-msg-1",
                  "handle": "\\@product-cards--devices",
                  "section": null,
                  "carousel": true
                }
              ]
            }
          },
          {
            "name": "carousel-product-card--few-items",
            "data": {
              "status": {
                "container-id": "status-container-2",
                "sr-element-id": "status-msg-2",
                "sr-text": "Showing %{start} through %{end} of %{total} items",
                "visible-text": "Showing %{start}%{separator}%{end} of %{total} items"
              },
              "slides": [
                {
                  "aria-roledescription": "slide",
                  "aria-labelledby": "status-msg-2",
                  "handle": "\\@product-cards--4-apps",
                  "section": null,
                  "carousel": true
                }
              ]
            }
          },
          {
            "name": "highlight--right-aligned"
          },
          {
            "name": "area-heading--centered"
          },
          {
            "name": "content-cards-vertical--3-up-depth"
          },
          {
            "name": "area-heading--with-paragraph"
          },
          {
            "name": "content-cards-vertical--4-up-depth"
          },
          {
            "name": "back-to-top--standalone",
            "container-classes": "mb-0"
          }
        ]
      }
    }
  ]
}
